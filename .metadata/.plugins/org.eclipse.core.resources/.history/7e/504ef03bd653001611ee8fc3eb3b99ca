import java.util.ArrayList;
import java.util.Arrays;
import java.util.PriorityQueue;
import java.util.Scanner;

public class source {
	
	static final int N = 5842;
	static int[] D = new int[N]; 
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		PriorityQueue<Integer> queue2 = new PriorityQueue<Integer>(); 
		PriorityQueue<Integer> queue3 = new PriorityQueue<Integer>(); 
		PriorityQueue<Integer> queue5 = new PriorityQueue<Integer>(); 
		PriorityQueue<Integer> queue7 = new PriorityQueue<Integer>(); 
		
		queue2.add(1);
		queue3.add(1);
		queue5.add(1);
		queue7.add(1);

		int i = 0;
		int v2,v3,v5,v7;

		v2 = queue2.poll() * 2;
		v3 = queue3.poll() * 3;
		v5 = queue5.poll() * 5;
		v7 = queue7.poll() * 7;

		do {
			switch(min(v2,v3,v5,v7)) {
			case 0 :
				queue2.add(2);
				break;
			case 1 : 
				break;
			case 2 : 
				break;
			case 3 : 
				break;
			default: break;
			}
			
		} while(i < N);
		
	}
	
	public static int min(int v1, int v2, int v3, int v4) {
		
		int[] v = {v1,v2,v3,v4};
		
		Arrays.sort(v);
		
		if(v[0] == v1) return 0;
		else if(v[0] == v2) return 1;
		else if(v[0] == v3) return 2;
		else if(v[0] == v4) return 3;
		else return -1;
	}
	
